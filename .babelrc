{
  "plugins": ["transform-object-rest-spread", "react-hot-loader/babel", ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]],
  "env": {
    "development": {
      "presets": ["react", "env"]
    },
    "production": {
      "presets": [
        "react",
        [
          "env",
          {
            "modules": false,
            "browsers": ["last 2 versions", "ie >= 11"]
          }
        ]
      ]
    }
  }
}